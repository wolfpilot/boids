{
  "extends": "tslint:recommended",
  "rules": {
    "prettier": true,
    "no-arg": true,
    "no-bitwise": true,
    "no-conditional-assignment": true,
    "no-console": {
      "severity": "warning",
      "options": ["debug", "info", "log", "time", "timeEnd", "trace"]
    },
    "ordered-imports": false,
    "no-namespace": [true, "allow-declarations"],
    "object-literal-sort-keys": false
  }
}
